nd:#fff;\n    //height:150px;\n    border-radius:20px;\n    color:#000;\n    box-shadow:0 4px 15px rgba(0,0,0,0.15);\n    display:inline-block;\n    margin-left:70px;\n    white-space:wrap;\n    @media screen and (max-width: 480px) {\n        margin-left:30px;\n        width:75%;\n    }\n    \n  \n`\n\nconst Picture = styled.div`\n    width:30%;\n    height:200px;\n    //background:red;\n    background-size:cover;\n   \n    float:left;\n    border-top-left-radius:20px;\n    border-bottom-left-radius:20px;\n`\nconst Text = styled.div`\n    width:70%;\n    display:inline-block;\n    padding:30px;\n    white-space:wrap;\n`\n\nconst Topic = styled.h2`\n      text-align:center;\n      font-size:20px;\n      color:#000;\n      font-weight:600px;\n      margin-bottom:30px;\n      @media screen and (max-width: 480px) {\n          font-size:15px;\n          font-weight:600;\n      }\n\n`\nfunction Testimonials() {\n    return (\n        <Container>\n            <Topic>Join the 5,000+ online course creators<br /> already growing with us.</Topic>\n            <Slide>\n                {Testimonies.map((item) => {\n                    return(\n                        <>\n                            <Slides>\n                                <Picture  style={{backgroundImage:`url(${item.image})`}}>\n                                    \n                                </Picture>\n                           \n                            <Text>\n                            <p>{item.testimony}</p>\n                                <p>{item.name}</p>\n                                <p>{item.country}</p>\n                            </Text>\n                            </Slides>\n                            \n                        </>\n                    )\n                })}\n            </Slide>\n            \n        </Container>\n    )\n}\n\nexport default Testimonials\n","/Users/apple/Documents/talcube-course-website/src/components/Faq.js",[],"/Users/apple/Documents/talcube-course-website/src/components/Data.js",[],"/Users/apple/Documents/talcube-course-website/src/components/PricingHome.js",[],"/Users/apple/Documents/talcube-course-website/src/components/PricingPage.js",["105","106","107","108","109"],"import React, {useState, useEffect} from 'react'\nimport styled from \"styled-components\"\nimport { Link } from \"react-router-dom\"\nimport {Prices} from \"./Data\"\nimport { GetEbookModal } from './Modal'\n\nconst Container = styled.div`\n    width:100%;\n    height:auto;\n    margin-top:80px;\n    margin-bottom:40px;\n`\nconst Pricing = styled.div`\n   width:100%;\n    height:auto;\n    display:flex;\n    flex-wrap:wrap;\n`\nconst Price = styled.div`\n    min-width:30%;\n    box-shadow:0 0 5px rgba(0,0,0,0.2);\n    margin-left:30px;\n    padding:30px;\n    border-radius:10px;\n    margin-bottom:30px;\n    text-align:center;\n    background-color:rgba(0,0,0,0.05);\n    h2{\n        font-weight:700;\n        font-size:25px;\n    }\n    p{\n        font-size:14px;\n        margin:20px 0;\n    }\n    select{\n        width:100px;\n        background-color:transparent;\n        outline:none;\n        margin-left:5px;\n        border:1px solid #CED4DA;\n        border-radius:3px;\n        padding:0 5px;\n    }\n    h1{\n        font-size:25px;\n        margin:30px 0;\n        color:#333;\n    }\n    h3{\n        margin-top:30px;\n        font-size:15px;\n        font-weight:600;\n        text-align:left;\n\n    }\n    li{\n        list-style-type:square;\n        font-size:14px;\n        text-align:left;\n        margin-bottom:12px;\n    }\n    :nth-child(2){\n        background-color:#00A650;\n        color:#fff;\n        margin-top:-30px;\n        button{\n            background-color:#f0f0f0; \n            color:#00A650\n        }\n    }\n    @media screen and (max-width: 480px) {\n        :nth-child(2){\n            margin-top:0;\n        }\n    }\n`\nconst Button = styled.button`\n   width:70%;\n    border-radius:7px;\n    color:#fff;\n    outline:none;\n    border:none;\n    background-color:#00A650;\n    height:40px;\n    font-weight:600;\n    text-decoration:none;\n`\nfunction PricingPage() {\n\n    const [priced, setPriced] = useState(Prices)\n    const [module, setModule] = useState(1)\n\n    const [showModal, setShowModal] = useState(false)\n\n    const openModal = () => {\n        setShowModal(prev => prev)\n    }\n\n\n    return (\n        <Container className=\"container\">\n            <Pricing>\n                {priced.map(item => {\n                    return(\n                        <Price>\n                            <h2>{item.type}</h2>\n                            <p>{item.text}</p>\n                            Module Number :\n                            <select value= {`${module}`} onChange={(e)=> setModule(parseInt(e.target.value))}>\n                                <option value=\"1\">1</option>\n                                <option value=\"2\">2</option>\n                                <option value=\"3\">3</option>\n                                <option value=\"4\">4</option>\n                                <option value=\"5\">5</option>\n                                <option value=\"6\">6</option>\n                            </select>\n                            <h1>${item.price}/Module</h1>\n                            <h2>Total: ${item.price * module}</h2>\n                            <Button><GetEbookModal totalPrice={item.price * module}/></Button>\n                            {/* <RegisterModal showModal={showModal} setShowModal={setShowModal} /> */}\n\n                            <h3>{item.benefitTopic}</h3>\n                            {item.benefits.map(benefits => (\n                                <li>{benefits}</li>\n                            ))}\n                        </Price>\n                    )\n                })}\n\n            </Pricing>\n            \n        </Container>\n    )\n}\n\nexport default PricingPage\n","/Users/apple/Documents/talcube-course-website/src/components/Modal.js",["110","111","112"],"import React, { useState, useEffect } from \"react\";\nimport styled from \"styled-components\";\nimport { ToastContainer, toast } from \"react-toastify\";\nimport \"react-toastify/dist/ReactToastify.css\";\nimport { Modal, Button } from \"antd\";\nimport  \"../styles/style.css\"\nimport UsingHooks from \"../components/PaymentHook\"\nimport useForm from \"./useForm\"\nimport validate from \"./validationInfo\"\n\n\nconst GetEbook = styled.div`\n  form{\n    h2{\n      font-size:16px;\n      font-weight:700;\n      margin-bottom:15px;\n    }\n    .form-group{\n      margin-bottom:25px;\n      label{\n        font-size:14px;\n        font-weight:500;\n      }\n      input,textarea{\n        font-size:15px;\n        ::placeholder{\n          font-size:13px;\n        }\n      }\n      \n    }\n  }\n`\n\nexport const RegisterModal = ({submitForm}) => {\n  //Visible modal\n  const [isModalVisible, setIsModalVisible] = useState(false);\n\n  const showModal = () => {\n    setIsModalVisible(true);\n  };\n\n  const handleOk = () => {\n    setIsModalVisible(false);\n  };\n\n  const handleCancel = () => {\n    setIsModalVisible(false);\n  };\n\n  \nconst {handleChange, values, handleSubmit, errors} = useForm(submitForm,validate)\n \n  return (\n    <>\n      <Button type=\"primary\" onClick={showModal}>\n        Get Now\n      </Button>\n      <Modal\n        title=\"Register To Get Ebook\"\n        visible={isModalVisible}\n        onOk={handleOk}\n        onCancel={handleCancel}\n      >\n        <div className=\"container-fluid\">\n          <div>\n            <form onSubmit={handleSubmit}>\n            <input\n              className=\"form-control\"\n              type=\"text\"\n              name=\"fullname\"\n              placeholder=\"Fullname\"\n              value={values.fullname}\n              onChange={handleChange}\n            />\n            {errors.fullname && <p style={{color:\"red\", font