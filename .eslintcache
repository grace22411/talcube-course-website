}) => {\n    return ( \n        <Navbar >\n        <Logo to=\"/\"><img src={logo} alt=\"logo image\" style={{height:\"70px\"}} ></img> </Logo> \n        <Bars  onClick={toggle}/>\n        <NavItems>\n            <NavLinks to=\"/\">Home</NavLinks>\n            <NavLinks to=\"/course\">Free Resources</NavLinks>\n            <NavLinks to=\"/pricing\">Pricing</NavLinks>\n            <NavLinks to=\"/contact\">Contact</NavLinks>\n        </NavItems>\n        \n        </Navbar>\n    )\n}\n\nexport default Header","/Users/apple/Documents/talcube-course-website/src/pages/Home.js",["102"],"\nimport React, {useState} from 'react'\nimport Caption from '../components/Caption'\nimport Endorsement from '../components/Endorsement'\nimport Faq from '../components/Faq'\nimport PageFooter from '../components/PageFooter'\nimport PricingHome from '../components/PricingHome'\n\nimport Testimonials from '../components/Testimonials'\nimport WhatWeDo from '../components/WhatWeDo'\n\nconst Home = () => {\n  \n    return (\n        <div>\n            \n            <Caption />\n            <Endorsement />\n            <WhatWeDo />\n            <Testimonials />\n            <Faq />\n            <PricingHome />\n            <PageFooter />\n        </div>\n    )\n}\n\nexport default Home\n","/Users/apple/Documents/talcube-course-website/src/pages/Contact.js",[],"/Users/apple/Documents/talcube-course-website/src/pages/Pricing.js",[],"/Users/apple/Documents/talcube-course-website/src/components/Caption.js",[],"/Users/apple/Documents/talcube-course-website/src/components/Endorsement.js",[],"/Users/apple/Documents/talcube-course-website/src/components/WhatWeDo.js",["103"],"import React from 'react'\nimport styled , {css} from \"styled-components\"\nimport what from \"../images/what.png\"\nimport creation from \"../images/mdi_creation.png\"\nimport outlined from \"../images/ant-design_ant-design-outlined.png\"\nimport carbon from \"../images/carbon_machine-learning-model.png\"\nimport advertise from 